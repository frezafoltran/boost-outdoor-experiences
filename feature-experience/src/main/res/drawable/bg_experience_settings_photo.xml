    <item>
        <shape android:shape="rectangle">
            <stroke android:width="3dp" android:color="@color/primary" />
        </shape>
    </item>
    <item android:left="3dp" android:right="3dp"  android:top="3dp"  android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/translucent" />
        </shape>
    </item>
</layer-list>